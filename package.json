    "gh": "node ./_fastboot/gh",
    "gh-push": "node ./_fastboot/gh-push",
    "npm": "node ./_fastboot/npm",
    "s": "node _fastboot/server",
    "test": "mocha --timeout 60000"
  },
  "main": "index.js",
  "keywords": [
    "fast-flow",
    "timer",
    "countdown"
  ],
  "repository": {
